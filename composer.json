{
    "name": "sebastopolys/smartlink-dynamic-urls",
    "description": "A new concept that redefines the meaning of web link",
    "type": "composer-plugin",
    "license": "gnu",
    "autoload": {
        "psr-4": {
            "Smrtdu\\SmartlinkDinamicUrls\\": "src/"
        }
    },
    "authors": [
        {
            "name": "sebastopolys",
            "email": "sebastopolys@gmail.com"
        }
    ],
    "minimum-stability": "beta",
	"keywords": [
		"wordpress",
		"openid"
	],
	"config": {
		"platform": {
			"php": "7.4"
		},
		"optimize-autoloader": true,
        "allow-plugins": {
            "composer/installers": true,
            "dealerdirect/phpcodesniffer-composer-installer": true
        }
	},
	"repositories": [
		{
			"type": "git",
			"url": "https://github.com/sebastopolys/smartlink-dynamic-urls"
            
		}
	],
    "require": {}
}
